<Window x:Class="PT_LAB.PasswordDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Enter Password" Height="150" Width="400">
    <StackPanel Margin="10">
        <TextBlock Text="Enter Password:" Margin="0,0,0,10"/>
        <PasswordBox Name="PasswordBox" Margin="0,0,0,10"/>
        <Button Content="OK" Width="75" HorizontalAlignment="Right" Click="OK_Click"/>
    </StackPanel>
</Window>
